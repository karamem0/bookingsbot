{
  "type": "AdaptiveCard",
  "version": "1.3",
  "body": [
    {
      "type": "FactSet",
      "facts": [
        {
          "title": "ビジネス",
          "value": "${BusinessName}"
        },
        {
          "title": "サービス",
          "value": "${ServiceName}"
        },
        {
          "title": "開始時刻",
          "value": "{{DATE(${StartTime},SHORT)}} {{TIME(${StartTime})}}"
        },
        {
          "title": "終了時刻",
          "value": "{{DATE(${EndTime},SHORT)}} {{TIME(${EndTime})}}"
        },
        {
          "title": "名前",
          "value": "${CustomerName}"
        },
        {
          "title": "電子メール アドレス",
          "value": "${CustomerEmail}"
        }
      ]
    }
  ]
}
